    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <size android:width="@dimen/dimen_24" android:height="@dimen/dimen_24" />
            <stroke android:width="1dp" android:color="@color/n4" />
        </shape>
    </item>
</selector>
